{
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "AspNetCoreRequestTracing": "Trace"
      }
    },
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "RequestTracingMiddlewareOptions": {
    "EnableFor": [
      "/api/.*"
    ]
  }
}
